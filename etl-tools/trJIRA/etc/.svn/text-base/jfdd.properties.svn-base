# Properties file for JFDD

# JIRA
------
base.url=http://localhost:8080/rpc/soap/jirasoapservice-v2
jira.username=myjirausername
jira.password=myjirapassword

# This must match the filter ID for the JIRA items you want to process.
jira.filter=10000

# This the default time (in seconds) for a task if an 
# original estimate is not provided in JIRA.
default.estimate=432000


# FDDPMA Database
-----------------
db.username=mydbusername
db.password=mydbpassword
db.url=jdbc:mysql://localhost:3306/fddpma
db.driver=com.mysql.jdbc.Driver